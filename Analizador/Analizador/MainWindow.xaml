<Window x:Class="Analizador.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Analizador"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="584">
    <Grid Margin="0,0,0,-16" HorizontalAlignment="Center" Width="589">
        <Grid.Background>
            <ImageBrush ImageSource="/23627.jpg"/>
        </Grid.Background>
        <Label Content="Ingrese la expresion:" HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="167" Margin="24,36,0,0" FontFamily="Comic Sans MS" Foreground="#FFBDC9FF" FontSize="16"/>
        <TextBox x:Name="expresiontxt" HorizontalAlignment="Left" Margin="213,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="279" Background="#FFB6B6B6" FontFamily="Comic Sans MS"/>
        <Button Content="Analizar" HorizontalAlignment="Center" Margin="0,85,0,0" VerticalAlignment="Top" Width="96" Background="#FFC1C1C1" Click="Button_Click"/>
        <ListView x:Name="Lista" Margin="96,132,96,26">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
